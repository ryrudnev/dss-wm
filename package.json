{
  "name": "dss-wm",
  "version": "0.0.1",
  "description": "Simple realization an intelligent decision support system in a waste management",
  "scripts": {
    "slate": "rimraf node_modules && npm install",
    "start": "cross-env NODE_ENV=development && DEBUG=server,server:* node bin/server.js",
    "start-prod": "cross-env NODE_ENV=production node bin/server.js",
    "api": "cross-env NODE_ENV=development && DEBUG=api,api:* nodemon --watch api bin/api.js",
    "api-prod": "cross-env NODE_ENV=production node bin/api.js",
    "api-debug": "cross-env NODE_ENV=development && DEBUG=api,api:* concurrently --kill-others \"nodemon --debug --watch api bin/api.js\" \"node-inspector -p 3020\"",
    "build": "cross-env NODE_ENV=production webpack --progress --verbose --display-error-details --config webpack/webpack.config.js",
    "build-analyze": "cross-env NODE_ENV=production webpack --json --config bin/webpack.config.js | webpack-bundle-size-analyzer > logs/build-analyze.log",
    "test-api": "cross-env NODE_ENV=test node_modules/mocha/bin/mocha --compilers js:babel-core/register api/tests/**/*.spec.js",
    "test-api-watch": "npm run test-api -- --watch",
    "coverage-api": "rimraf coverage && cross-env NODE_ENV=test babel-node node_modules/isparta/bin/isparta cover --report text --report html  node_modules/mocha/bin/_mocha -- --reporter spec api/tests/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryrudnev/dss-wm.git"
  },
  "keywords": [
    "dss",
    "stardog",
    "nodejs",
    "bootstrap",
    "backbone",
    "react"
  ],
  "author": "Roman Rudnev<ryrudnev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryrudnev/dss-wm/issues"
  },
  "homepage": "https://github.com/ryrudnev/dss-wm",
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone-associations": "^0.6.2",
    "backbone.base-router": "^1.3.0",
    "backbone.radio": "^1.0.5",
    "bcrypt": "^0.8.5",
    "body-parser": "~1.15.0",
    "bootstrap-sass": "^3.3.6",
    "bootswatch": "^3.3.6",
    "connect-roles": "^3.1.2",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-unless": "^0.3.0",
    "helmet": "^2.0.0",
    "i18n": "^0.8.2",
    "jsonwebtoken": "^5.7.0",
    "md5": "^2.1.0",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.12",
    "morgan": "^1.7.0",
    "object-path": "^0.9.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "react": "^15.0.2",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.0.2",
    "stardog": "^0.3.1",
    "store": "^1.3.20",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "~6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "clean-webpack-plugin": "^0.1.9",
    "concurrently": "^2.1.0",
    "copy-webpack-plugin": "^3.0.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-mocha": "^2.2.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "http-proxy-middleware": "^0.15.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "supertest": "^1.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^0.12.0"
  }
}
