{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
   },
   "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true,
    "modules": true
   },
   "rules": {
     "consistent-return": 0,
     "no-underscore-dangle": 0,
     "no-param-reassign": 0,
     "no-nested-ternary": 0
   }
}
